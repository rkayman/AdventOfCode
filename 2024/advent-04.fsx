let findAll x xs=
    xs
    |> Array.indexed
    |> Array.rev
    |> Array.fold (fun acc (i, x') -> if x = x' then i::acc else acc) []
    |> List.toArray

let findSimple (word: char array) rowLength (xs: char array) =
    let cw = word |> Array.length
    let isMatch i =
        let row = i / rowLength
        let rowEnd = (row + 1) * rowLength
        if i + cw > rowEnd then
            false
        else
            let iz = i + cw - 1
            (word, xs[i..iz]) ||> Array.forall2 (=)
    xs
    |> findAll word[0]
    |> Array.filter isMatch
    
let findDiagonal (word: char array) rowLength (xs: char array) =
    let cw = (word |> Array.length) - 1
    let rowCount = xs.Length / rowLength 
    
    let indices i =
        let row = i / rowLength
        let col = i % rowLength
        seq {
            
            // Northwest
            if row >= cw && col >= cw then
                yield [| for j in 1..cw -> i - j * (rowLength + 1) |]
            // Northeast
            if row >= cw && col < rowLength - cw then
                yield [| for j in 1..cw -> i - j * (rowLength - 1) |]
            // Southeast
            if row < rowCount - cw && col < rowLength - cw then
                yield [| for j in 1..cw -> i + j * (rowLength + 1) |]
            // Southwest
            if row < rowCount - cw && col >= cw then
                yield [| for j in 1..cw -> i + j * (rowLength - 1) |]
        }

    let isMatch (ixs: int array) =
        ixs
        |> Array.map (fun x -> xs[x])
        |> Array.forall2 (=) (word[1..])
            
    xs
    |> findAll word[0]
    |> Seq.collect indices
    |> Seq.filter isMatch

let wordSearch word xss =
    let rowLength = xss |> Array.length
    
    let xs = xss |> Array.concat
    let cntForward = xs |> findSimple word rowLength
    let cntBackward = xs |> Array.rev |> findSimple word rowLength
    
    let xss' = xss |> Array.transpose
    let xs' = xss' |> Array.concat
    let cntDown = xs' |> findSimple word rowLength
    let cntUp = xs' |> Array.rev |> findSimple word rowLength

    let cntDiagonal = xs |> findDiagonal word rowLength |> Seq.length
    
    let cntSimple = 
        [cntForward; cntBackward; cntDown; cntUp]
        |> Seq.map Seq.length
        |> Seq.sum
        
    printfn $"Total count: %d{cntSimple + cntDiagonal}"
    
let findXmas rowLength (xs: char array) =
    let word = "MAS".ToCharArray()
    let word' = word |> Array.rev 
    let rowCount = xs.Length / rowLength
    
    let indices idx =
        let row = idx / rowLength
        let col = idx % rowLength
        seq {
            if row > 0 && col > 0 then
                if row < rowCount - 1 && col < rowLength - 1 then
                    yield
                        [| for i in -1..1 -> idx + (i * (rowLength + 1)) |],
                        [| for i in -1..1 -> idx - (i * (rowLength - 1)) |]
                        |> Array.rev 
        }

    let isMatch ixs =
        let is, js = ixs
        let criss = is |> Array.map (fun x -> xs[x])
        let cross = js |> Array.map (fun x -> xs[x])
        (criss = word || criss = word') && (cross = word || cross = word')
        
    xs
    |> findAll 'A'
    |> Seq.collect indices
    |> Seq.filter isMatch

let xmasSearch xss =
    let rowLength = xss |> Array.length    
    let xs = xss |> Array.concat

    let cntCrissCross = xs |> findXmas rowLength |> Seq.length
    
    printfn $"Total count: %d{cntCrissCross}"

    
